    "debug": "^2.2.0",
    "express": "^4.14.0",
    "express-winston": "^2.4.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "figures": "^2.0.0",
    "file-loader": "^0.9.0",
    "fs-extra": "^1.0.0",
    "imports-loader": "^0.6.5",
    "ip": "^1.1.2",
    "json-loader": "^0.5.4",
    "lodash": "^4.17.4",
    "node-sass": "^4.5.3",
    "postcss-loader": "^2.0.5",
