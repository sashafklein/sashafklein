{
  "name": "rs-starter",
  "version": "3.0.0-alpha.2",
  "description": "RS Starter app",
  "main": "index.js",
  "engines": {
    "node": "^6.11.1",
    "npm": "^3.0.0"
  },
  "scripts": {
    "clean": "rimraf dist",
    "compile": "node build/scripts/compile",
    "build": "npm run clean && env NODE_ENV=production npm run compile",
    "start": "NODE_ENV=development node build/scripts/start.js",
    "start:prod": "yarn run build && env NODE_ENV=production node bin/server.js",
    "test": "env NODE_ENV=test karma start config/karma.config",
    "test:watch": "npm test -- --watch",
    "test:full": "yarn test && yarn lint && yarn docs",
    "lint": "eslint . --quiet",
    "lint:all": "eslint .",
    "lint:fix": "eslint . --fix --quiet",
    "lint:sass": "sass-lint --verbose",
    "postinstallOnly": "npm rebuild node-sass"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sashafklein/sashafklein.git"
  },
  "author": "Sasha Klein <sashafklein@gmail.com>",
  "license": "MIT",
  "dependencies": {
